  "bugs": {
    "url": "https://github.com/Reviewable/fireplan/issues"
  },
  "homepage": "https://github.com/Reviewable/fireplan",
  "dependencies": {
    "clone": "2",
    "escodegen": "2",
    "esprima": "4",
    "estraverse": "5.3.0",
